CREATE TABLE IF NOT EXISTS song(
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    title TEXT NOT NULL,
    album TEXT,
    artist TEXT NOT NULL,
    path TEXT,
    duration INTEGER,
    status TEXT
);

selectAll:
SELECT * FROM song;

insert:
INSERT INTO song(title, album, artist, path, duration, status) VALUES(?, ?, ?, ?, ?, ?);
