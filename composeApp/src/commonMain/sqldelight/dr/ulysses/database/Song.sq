CREATE TABLE IF NOT EXISTS song(
    path TEXT PRIMARY KEY NOT NULL,
    title TEXT NOT NULL,
    album TEXT,
    artist TEXT NOT NULL,
    duration INTEGER,
    state TEXT
);

selectAll:
SELECT * FROM song;

insert:
INSERT INTO song(title, album, artist, path, duration, state) VALUES(?, ?, ?, ?, ?, ?);

deleteByPath:
DELETE FROM song WHERE path = ?;
